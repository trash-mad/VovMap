<Window x:Class="VovMap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:pageTransitions="clr-namespace:WpfPageTransitions;assembly=WpfPageTransitions"
        xmlns:local="clr-namespace:VovMap"
        Background="White"
        mc:Ignorable="d"
        Closing="Window_Closing"
        Title="MainWindow" Height="350" Width="525">
    <Window.ContextMenu>
        <ContextMenu>
            <MenuItem Header="На весь экран/окно" Click="MenuItem_Click"/>
        </ContextMenu>
    </Window.ContextMenu>
    <pageTransitions:PageTransition Name="PageControl" Margin="0" TransitionType="SlideAndFade" />

</Window>
